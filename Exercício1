package elenco;
import java.util.Scanner;
	
public class FelipeLongo {
	
	public static void main(String[] args) {
			
		Scanner ler = new Scanner(System.in);
		String a;
			
			
			
		for (int i = 0 ; i < 5; i++) {
			System.out.print("Informe seu nome:");
			a = ler.next();
			System.out.println(a);
			System.out.println("==================");
			}
		ler.close();
		}
			
	
	}
	
	
